<Window x:Class="ST10398576_PROG6212_POE.LecturerDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ST10398576_PROG6212_POE"
        mc:Ignorable="d"
            Title="LecturerDashboard" Height="450" Width="500">
    <!-- Link to GitHub Repository:https://github.com/ST10398576/ST10398576_PROG6212_POE.git -->
    <Grid Background="#D8DEE7">
        <Button x:Name="btnSubmitClaim" Content="Submit Claim" HorizontalAlignment="Center" Margin="0,67,0,0" VerticalAlignment="Top" Height="39" Width="396" Click="btnSubmitClaim_Click"/>

        <Button x:Name="btnViewClaims" Content="View Claims" HorizontalAlignment="Center" Margin="0,133,0,0" VerticalAlignment="Top" Height="39" Width="396" Click="btnViewClaims_Click" />

        <Button x:Name="btnReturn" Content="Return to Main Menu" HorizontalAlignment="Center" VerticalAlignment="Top" Height="39" Width="242" Click="btnReturn_Click" Margin="0,239,0,0" />

    </Grid>
</Window>
